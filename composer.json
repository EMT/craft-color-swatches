  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "color-swatches"
  ],
  "support": {
    "docs": "https://github.com/Rias500/craft-color-swatches/blob/master/README.md",
    "issues": "https://github.com/Rias500/craft-color-swatches/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rias",
      "homepage": "https://rias.be"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0"
  },
  "autoload": {
