      "rias\\colorswatches\\": "src/"
    }
  },
  "extra": {
    "name": "Color Swatches",
    "handle": "color-swatches",
    "schemaVersion": "1.0.0",
    "hasCpSettings": false,
    "hasCpSection": false,
